{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended",
    "prettier/react"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "quotes": [2, "single"]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
